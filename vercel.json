{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.3.1"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "prod",
        "APP_DEBUG": "true"
    }
}